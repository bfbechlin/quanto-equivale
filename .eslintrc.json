{
    "extends": "airbnb",
    "globals": {
        "document": true,
        "window": true,
        "navigator": true
    },
    "parser": "babel-eslint",
    "rules": {
        "react/prop-types": 0,
        "indent": ["error", 2],
        "no-nested-ternary": 0,
        "no-unused-vars": [
            "error", 
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": false, "argsIgnorePattern": "[theme]" }
        ]
    }
}